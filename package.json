{
    "name": "vscode-angular2-files",
    "displayName": "Angular2 Files",
    "description": "Quickly add angular 2 files using angular-cli",
    "version": "1.3.0",
    "icon": "images/icon.png",
    "publisher": "alexiv",
    "author": {
        "name": "Alexander Ivanichev"
    },
    "engines": {
        "vscode": "^1.5.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/qwert789/vscode-angular2-files"
    },
    "bugs": {
        "url": "https://github.com/qwert789/vscode-angular2-files/issues"
    },
    "categories": [
        "Other",
        "Languages",
        "Snippets"
    ],
    "scripts": {
        "vscode:prepublish": "node ./node_modules/vscode/bin/compile",
        "compile": "node ./node_modules/vscode/bin/compile -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "activationEvents": [
        "*"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.addAngular2Component",
                "title": "New Component (Angular2)"
            },
            {
                "command": "extension.addAngular2Directive",
                "title": "New Directive (Angular2)"
            },
            {
                "command": "extension.addAngular2Pipe",
                "title": "New Pipe (Angular2)"
            },
            {
                "command": "extension.addAngular2Service",
                "title": "New Service (Angular2)"
            },
            {
                "command": "extension.addAngular2Class",
                "title": "New Class (Angular2)"
            },
            {
                "command": "extension.addAngular2Interface",
                "title": "New Interface (Angular2)"
            },
            {
                "command": "extension.addAngular2Enum",
                "title": "New Enum (Angular2)"
            },
            {
                "command": "extension.addAngular2Module",
                "title": "New Module (Angular2)"
            }
        ],
        "menus": {
            "explorer/context": [
                {
                    "when": "",
                    "command": "extension.addAngular2Component",
                    "group": "Angular2"
                },
                {
                    "when": "",
                    "command": "extension.addAngular2Directive",
                    "group": "Angular2"
                },
                {
                    "when": "",
                    "command": "extension.addAngular2Pipe",
                    "group": "Angular2"
                },
                {
                    "when": "",
                    "command": "extension.addAngular2Service",
                    "group": "Angular2"
                },
                {
                    "when": "",
                    "command": "extension.addAngular2Module",
                    "group": "Angular2"
                },
                {
                    "when": "",
                    "command": "extension.addAngular2Class",
                    "group": "Angular2 Essentials"
                },
                {
                    "when": "",
                    "command": "extension.addAngular2Interface",
                    "group": "Angular2 Essentials"
                },
                {
                    "when": "",
                    "command": "extension.addAngular2Enum",
                    "group": "Angular2 Essentials"
                }
            ]
        }
    },
    "devDependencies": {
        "typescript": "^1.8.5",
        "rimraf": "^2.4.4",
        "vscode": "^0.11.0"
    },
    "dependencies": {
        "q": "^1.4.1"
    }
}